<!DOCTYPE html>
<html lang="en">

<head>
    {% include "~bdo/views/head.njk" %}
    {% if process.env.NODE_ENV === 'development' %}
    <script src="http://localhost:35729/livereload.js"></script>
    <script src="/js/browser-source-map-support.js"></script>
    <script>sourceMapSupport.install();</script>
    {% endif %}
</head>

<body>
    <script>
        window.process = {{ process | json | safe }};
    </script>
    <a href="/" is="view-link">GO HOME</a>
    <a href="/config/config" is="view-link">GO CONFIG!!!</a>

    <view-router>{% block body %}{% endblock %}</view-router>
    <script src="/js/bundle.js"></script>
</body>

</html>
