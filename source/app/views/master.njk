<!DOCTYPE html>
<html lang="en">

<head>
    {% include "~bdo/views/head.njk" %}
</head>

<body>
    <script nonce="{{ responseLocals.cspScriptNonce }}">
        window.process = JSON.parse('{{ process | json }}');
        window.cspScriptNonce = "{{ responseLocals.cspScriptNonce }}";
    </script>
    <view-router>{% block body %}{% endblock %}</view-router>
    {% if process.env.NODE_ENV !== 'development' %}
    <script defer src="/js/vendor.bundle.js"></script>
    <script defer src="/js/lib.bundle.js"></script>
    <script defer src="/js/styles.bundle.js"></script>
    <script defer src="/js/templates.bundle.js"></script>
    {% endif %}
    <script defer src="/js/bundle.js"></script>
</body>

</html>
